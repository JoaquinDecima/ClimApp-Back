{
  "name": "clima-back",
  "version": "1.0.0",
  "description": "Back para app de Clima desarrollada por Challenge de TECO",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "test": "jest --detectOpenHandles"
  },
  "keywords": [
    "clima",
    "js",
    "app",
    "teco"
  ],
  "author": "Joaquin (Pato)",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/supertest": "^2.0.10",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.+(ts|js)"
    ]
  }
}
